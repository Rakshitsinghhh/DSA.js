/**
 * @param {number} n
 * @param {number} m
 * @return {number}
 */
var differenceOfSums = function(n, m) {
        let divsum=0;
        let ndivsum=0;
        for(let i=1;i<=n;i++)
        {
            if(i%m==0)
            {
                divsum+=i;
            }
            else{
                ndivsum+=i;
            }
        }
        return ndivsum-divsum;

    
};
