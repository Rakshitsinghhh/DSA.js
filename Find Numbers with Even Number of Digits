/**
 * @param {number[]} nums
 * @return {number}
 */

function sizer(a)
{
    const ans=String(a);
    return ans.length;
}
var findNumbers = function(nums) {
    let count=0;
    for(let i=0;i<nums.length;i++)
    {
        let size=sizer(nums[i])
        if(size%2==0)
        {
            count+=1;
        }
    }
    return count;
    
};
